{
  "name": "aluna.js",
  "version": "0.0.1",
  "description": "Aluna exchanges hub.",
  "main": "dist/src/index.js",
  "author": "Anderson Arboleya<anderson@aluna.social>",
  "contributors": [
    "Sergio Torres<sergio@aluna.social>"
  ],
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "^16.13"
  },
  "scripts": {
    "lintfix": "eslint --fix --ext .js,.ts . --max-warnings=0",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn build:index && tsc -p ./tsconfig.dist.json && yarn build:index:clean",
    "build:index": "yarn cti entrypoint ./src -s false -e 'exchanges' -i '.spec' -o index.ts -w -b",
    "build:index:clean": "rm -rf src/index.ts",
    "build:watch": "yarn clean && tsc -p ./tsconfig.dist.json -w",
    "test:jest": "jest --coverage=false",
    "test:jest:watch": "jest --watchAll --coverage=false",
    "test:jest:cover": "jest",
    "test:jest:cover:watch": "jest --watch",
    "test:mocha": "mocha",
    "test:mocha:watch": "mocha --watch",
    "test:mocha:cover": "nyc mocha --bail",
    "test:mocha:cover:watch": "nyc mocha --watch",
    "test:mocha:ci": "nyc mocha --bail --forbid-only"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.2",
    "fast-json-stable-stringify": "^2.1.0",
    "js-sha512": "^0.8.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "tslog": "^3.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.178",
    "@types/node": "^15.3.1",
    "@types/sinon": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "^4.3.4",
    "create-ts-index": "^1.14.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^7.0.4",
    "jest": "^27.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "ts-jest": "^27.0.3",
    "ts-mock-imports": "^1.3.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.4"
  }
}
